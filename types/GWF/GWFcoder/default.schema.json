{
  "$id": "https://akenza.io/gwf/gwfcoder/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "meterMedium": {
      "title": "Meter medium",
      "description": "Meter medium",
      "type": "string",
      "enum": [
        "GAS",
        "WARM_WATER",
        "WATER"
      ]
    },
    "actualityDuration": {
      "title": "Actuality duration",
      "type": "integer",
      "unit": "min",
      "description": "Actuality duration in min",
      "minimum": 0,
      "maximum": 65535
    },
    "volume": {
      "title": "Volume",
      "unit": "m^3",
      "type": "number",
      "description": "Tank volume in m3"
    },
    "absoluteVolume": {
      "title": "Absolute Volume",
      "unit": "m^3",
      "type": "number",
      "description": "Absolute tank volume in m3"
    }
  },
  "required": [
    "meterMedium",
    "actualityDuration",
    "volume"
  ]
}