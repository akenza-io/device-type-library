{
  "$id": "https://akenza.io/enless/600-039/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "ch1StateCounter": {
      "type": "integer",
      "title": "Channel 1 State Counter",
      "description": "Total number of state changes detected on channel 1",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/pulseInput"
    },
    "ch2StateCounter": {
      "type": "integer",
      "title": "Channel 2 State Counter",
      "description": "Total number of state changes detected on channel 2",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/pulseInput"
    },
    "ocStateCounter": {
      "type": "integer",
      "title": "Open Collector State Counter",
      "description": "Total number of state changes detected on the open collector input",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/pulseInput"
    },
    "ch1State": {
      "type": "string",
      "enum": ["open", "closed"],
      "title": "Channel 1 State",
      "description": "Digital state of input channel 1",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/digitalInput"
    },
    "ch2State": {
      "type": "string",
      "enum": ["open", "closed"],
      "title": "Channel 2 State",
      "description": "Digital state of input channel 2",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/digitalInput"
    },
    "ocState": {
      "type": "string",
      "enum": ["open", "closed"],
      "title": "Open Collector State",
      "description": "Digital state of the open collector input",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/ios/schema.json#/$defs/digitalInput"
    },
    "msgType": {
      "type": "string",
      "title": "Message Type",
      "enum": ["normal", "alarm"],
      "description": "Indicates whether the message was triggered by normal operation or an alarm"
    }
  },
  "required": [
    "ch1StateCounter",
    "ch2StateCounter",
    "ocStateCounter",
    "ch1State",
    "ch2State",
    "ocState",
    "msgType"
  ]
}
