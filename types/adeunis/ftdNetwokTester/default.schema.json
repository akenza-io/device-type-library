{
    "$id": "https://akenza.io/adeunis/ftd/default.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "processingType": "uplink_decoder",
    "topic": "default",
    "title": "Default",
    "properties": {
        "temperature": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius""title": "Temperature"
        },
        "trigger": {
            "title": "Trigger",
            "description": "Action which triggered the uplink",
            "type": "string",
            "enum": [
                "ACCELEROMETER",
                "PUSHBUTTON"
            ]
        },
        "uplink": {
            "title": "Uplink count",
            "description": "Count of triggered uplinks",
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        },
        "downlink": {
            "title": "Downlink count",
            "description": "Count of triggered downlinks",
            "type": "integer",
            "minimum": 0,
            "maximum": 255
        },
        "batteryVoltage": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/device/schema.json#/$defs/batteryVoltage/volt"
        },
        "batteryLevel": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/device/schema.json#/$defs/batteryLevel/percent"
        },
        "rssi": {
            "title": "RSSI",
            "unit": "dBm",
            "description": "Received Signal Strength Indication",
            "type": "number"
        },
        "snr": {
            "title": "SNR",
            "unit": "dB",
            "description": "Signal-to-Noise Ratio",
            "type": "number"
        }
    },
    "required": [
        "temperature",
        "trigger",
        "uplink",
        "downlink",
        "batteryVoltage",
        "batteryLevel"
    ]
}