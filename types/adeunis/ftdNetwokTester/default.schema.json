{
  "$id": "https://akenza.io/adeunis/ftd/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "temperature": {
      "title": "Temperature",
      "unit": "째C",
      "type": "number",
      "description": "The temperature in 째C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "trigger": {
      "title": "Trigger",
      "description": "Action which triggered the uplink",
      "type": "string",
      "enum": ["ACCELEROMETER", "PUSHBUTTON"]
    },
    "latitude": {
      "title": "Latitude",
      "unit": "째",
      "type": "number",
      "description": "Latitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "longitude": {
      "title": "Longitude",
      "unit": "째",
      "type": "number",
      "description": "Longitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "uplink": {
      "title": "Uplink count",
      "description": "Count of triggered uplinks",
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "downlink": {
      "title": "Downlink count",
      "description": "Count of triggered downlinks",
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "batteryVoltage": {
      "title": "Battery voltage",
      "unit": "V",
      "description": "Battery voltage",
      "type": "number",
      "minimum": 0,
      "maximum": 65535
    },
    "rssi": {
      "title": "RSSI",
      "unit": "dBm",
      "description": "Received Signal Strength Indication",
      "type": "number"
    },
    "snr": {
      "title": "SNR",
      "unit": "dB",
      "description": "Signal-to-Noise Ratio",
      "type": "number"
    }
  }
}
