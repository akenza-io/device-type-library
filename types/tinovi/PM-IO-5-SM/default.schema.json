{
  "$id": "https://akenza.io/decentlab/iam/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "valve": {
      "title": "Valve",
      "type": "boolean",
      "description": "Valve on = true, Valve off = false"
    },
    "leakDetected": {
      "title": "Leak detected",
      "type": "boolean",
      "description": "Leak detected, either true or false"
    },
    "soilConductivity": {
      "title": "Soil conductivity",
      "unit": "uS/cm",
      "type": "number",
      "description": "Conductivity of the soil"
    },
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "e25": {
      "title": "E25",
      "type": "number",
      "description": "E25"
    },
    "waterContent": {
      "title": "Water content",
      "unit": "VWC",
      "type": "number",
      "description": "Water content in the soil",
      "minimum": 0,
      "maximum": 100
    }
  }
}
