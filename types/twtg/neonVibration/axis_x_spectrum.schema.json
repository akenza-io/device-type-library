      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/speed/mPerS"
    },
    "rpm": {
      "title": "RPM",
      "type": "number",
      "description": "RPM"
    },
    "frequencies": {
      "title": "Frequencies",
      "type": "array",
      "description": "Frequencies",
      "hideFromKpis": true
    },
    "magnitudes": {
      "title": "Magnitudes",
      "type": "array",
      "description": "Magnitudes",
      "hideFromKpis": true
    }
  },
  "required": [
    "spectrumType",
    "temperature",
    "peakAcceleration",
    "rmsAcceleration",
    "rmsVelocity",
    "rpm",
    "frequencies",
    "magnitudes"
