      "description": "Spectrum type",
      "enum": [
        "ACCELERATION",
        "VELOCITY",
        "ENVELOPE"
      ]
    },
    "temperature": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
    },
    "peakAcceleration": {
      "title": "Peak acceleration",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/acceleration/mPerSsquared"
    },
    "rmsAcceleration": {
      "title": "RMS acceleration",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/acceleration/mPerSsquared"
    },
    "rmsVelocity": {
