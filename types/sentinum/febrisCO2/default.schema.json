{
  "$id": "https://akenza.io/sentinum/febris/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "humidity": {
      "title": "Humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "pressure": {
      "title": "Pressure",
      "unit": "hpa",
      "description": "Barometric Pressure in hpa",
      "type": "number",
      "minimum": 300,
      "maximum": 1100
    },
    "co2": {
      "title": "CO2",
      "unit": "ppm",
      "type": "integer",
      "description": "Carbon Dioxide in ppm",
      "minimum": 0,
      "maximum": 10000
    },
    "alarm": {
      "title": "Alarm",
      "description": "Alarm",
      "type": "string",
      "enum": [
        "GREEN",
        "YELLOW",
        "RED"
      ]
    },
    "dewPoint": {
      "title": "Dew point",
      "unit": "°C",
      "type": "number",
      "description": "Dew point  in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "wallTemperature": {
      "title": "Wall temperature",
      "unit": "°C",
      "type": "number",
      "description": "Wall temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "thermTemperature": {
      "title": "Thermometer temperature",
      "unit": "°C",
      "type": "number",
      "description": "Thermometer temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "wallHumidity": {
      "title": "Wall humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative wall humidity in %",
      "minimum": 0,
      "maximum": 100
    }
  },
  "required": [
    "humidity",
    "alarm"
  ]
}