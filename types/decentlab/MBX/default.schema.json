{
  "$id": "https://akenza.io/decentlab/mbx/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "distance": {
      "title": "Distance",
      "unit": "mm",
      "type": "number",
      "description": "Distance measured between the sensor and the surface"
    },
    "distanceCM": {
      "title": "Distance",
      "unit": "cm",
      "type": "number",
      "description": "Distance measured between the sensor and the surface"
    },
    "fillLevel": {
      "title": "Fill level",
      "unit": "%",
      "type": "number",
      "description": "Fill percentage of the container"
    }
  },
  "required": ["distance", "distanceCM"]
}
