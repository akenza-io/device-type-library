{
  "$id": "https://akenza.io/decentlab/lp8p/lifecycle.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lifecycle",
  "type": "object",
  "properties": {
    "batteryVoltage": {
      "title": "Battery voltage",
      "unit": "V",
      "type": "number",
      "minimum": 0,
      "maximum": 24,
      "desciption": "The battery voltage in Volt."
    },
    "protocolVersion": {
      "title": "Protocol Version",
      "description": "Protocol Version",
      "type": "integer",
      "hideFromKpis": true
    },
    "deviceID": {
      "title": "Device ID",
      "description": "Device ID",
      "type": "integer",
      "hideFromKpis": true
    },
    "co2SensorStatus": {
      "title": "CO2 sensor status",
      "type": "integer",
      "description": "Internal value for the co2 sensor status",
      "minimum": 0,
      "maximum": 255,
      "hideFromKpis": true
    }
  }
}
