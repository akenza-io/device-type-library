        },
        "alphaOuter": {
            "title": "Alpha (outer)",
            "description": "Alpha (outer)",
            "type": "number"
        },
        "alphaInner": {
            "title": "Alpha (inner)",
            "description": "Alpha (inner)",
            "type": "number"
        },
        "betaOuter": {
            "title": "Beta (outer)",
            "description": "Beta (outer)",
            "type": "number"
        },
        "betaInner": {
            "title": "Beta (inner)",
            "description": "Beta (inner)",
            "type": "number"
        },
