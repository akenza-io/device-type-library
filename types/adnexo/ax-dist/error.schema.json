{
  "$id": "https://akenza.io/adnexo/ax-dist/error.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "error",
  "title": "Error",
  "properties": {
    "errorOn": {
      "title": "Error occurance",
      "description": "Error occurance at following time",
      "type": "string",
      "enum": [
        "RESTART",
        "PARAM_CHANGE",
        "PARAM_READ"
      ],
      "hideFromKpis": true
    },
    "errorCode": {
      "title": "Error code",
      "description": "Error code",
      "type": "string",
      "enum": [
        "PARAM_UNKNOWN",
        "PARAM_READ_ONLY",
        "PARAM_NOT_READABLE",
        "COMMAND_STRUCTURE_WRONG"
      ],
      "hideFromKpis": true
    }
  },
  "required": [
    "errorOn",
    "errorCode"
  ]
}