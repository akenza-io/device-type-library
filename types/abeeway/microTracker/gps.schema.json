{
  "$id": "https://akenza.io/abeeway/microTracker/gps.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "gps",
  "title": "GPS",
  "properties": {
    "gpsMessage": {
      "title": "GPS Message",
      "description": "Flag that describes the following values",
      "type": "string",
      "enum": [
        "GPS_FIX",
        "GPS_FIX_EXTENDED"
      ],
      "hideFromKpis": true
    },
    "age": {
      "title": "Age",
      "unit": "s",
      "type": "integer",
      "description": "Age of the measurement in seconds",
      "hideFromKpis": true
    },
    "latitude": {
      "title": "Latitude",
      "unit": "°",
      "type": "number",
      "description": "Latitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "longitude": {
      "title": "Longitude",
      "unit": "°",
      "type": "number",
      "description": "Longitude",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "horizontalAccuracy": {
      "title": "Estimated horizontal position error",
      "unit": "m",
      "type": "number",
      "description": "Estimated horizontal position error in meters"
    },
    "dimensionFix": {
      "title": "Dimension fix",
      "description": "Describes the GPS fix",
      "type": "string",
      "enum": [
        "2D fix",
        "3D fix"
      ],
      "hideFromKpis": true
    },
    "cog": {
      "title": "Course over ground",
      "unit": "°",
      "type": "number",
      "description": "The direction the tracker is moving over the ground expressed in degrees.",
      "hideFromKpis": true
    },
    "sog": {
      "title": "Speed over ground",
      "unit": "cm/s",
      "type": "number",
      "description": "The speed the tracker is moving over the ground. It is expressed in cm/second."
    }
  },
  "required": [
    "gpsMessage",
    "age",
    "latitude",
    "longitude",
    "horizontalAccuracy"
  ]
}