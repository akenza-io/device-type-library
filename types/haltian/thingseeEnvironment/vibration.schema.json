{
  "$id": "https://akenza.io/haltian/thingseeAir/vibration.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Vibration",
  "type": "object",
  "properties": {
    "activityLevel": {
      "title": "Activity level",
      "type": "number",
      "description": "Percentage of detected activity during measurement period.",
      "unit": "%"
    },
    "energyLevel": {
      "title": "Energy level",
      "type": "integer",
      "description": "Cumulative sum of detected impact vector above threshold during measurement period."
    },
    "histogram0": {
      "title": "Histogram 0",
      "type": "integer",
      "description": "Count of impacts above approx. 128mG"
    },
    "histogram1": {
      "title": "Histogram 1",
      "type": "integer",
      "description": "Count of impacts above approx. 300mG"
    },
    "histogram2": {
      "title": "Histogram 2",
      "type": "integer",
      "description": "Count of impacts above approx. 500mG"
    },
    "histogram3": {
      "title": "Histogram 3",
      "type": "integer",
      "description": "Count of impacts above approx. 1G"
    },
    "histogram4": {
      "title": "Histogram 4",
      "type": "integer",
      "description": "Count of impacts above approx. 2G"
    },
    "histogram5": {
      "title": "Histogram 5",
      "type": "integer",
      "description": "Count of impacts above approx. 3G"
    },
    "histogram6": {
      "title": "Histogram 6",
      "type": "integer",
      "description": "Count of impacts above approx. 4G"
    },
    "histogram7": {
      "title": "Histogram 7",
      "type": "integer",
      "description": "Count of impacts above approx. 8G"
    },
    "histogram8": {
      "title": "Histogram 8",
      "type": "integer",
      "description": "Count of impacts above approx. 12G"
    },
    "histogram9": {
      "title": "Histogram 9",
      "type": "integer",
      "description": "Count of impacts above approx. 16G"
    }
  },
  "required": [
    "activityLevel",
    "energyLevel",
    "histogram0",
    "histogram1",
    "histogram2",
    "histogram3",
    "histogram4",
    "histogram5",
    "histogram6",
    "histogram7",
    "histogram8",
    "histogram9"
  ]
}
