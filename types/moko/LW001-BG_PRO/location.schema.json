{
    "type": "object",
    "properties": {
        "port": {
            "type": "number",
            "description": "Port number of the payload"
        },
        "hexFormatPayload": {
            "type": "string",
            "description": "Payload in hexadecimal format"
        },
        "payloadType": {
            "type": "string",
            "description": "Type of payload (e.g., Location Fixed)"
        },
        "operationMode": {
            "type": "string",
            "description": "Current operation mode of the device"
        },
        "temperature": {
            "type": "string",
            "description": "Temperature reading from the device"
        },
        "ack": {
            "type": "number",
            "description": "Acknowledgment value"
        },
        "batteryVoltage": {
            "type": "string",
            "description": "Battery voltage level"
        },
        "positionSuccessType": {
            "type": "string",
            "description": "Type of successful positioning (e.g., WIFI, Bluetooth, GPS)"
        },
        "timestamp": {
            "type": "string",
            "description": "Timestamp of the location data"
        },
        "macData": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "mac": {
                        "type": "string",
                        "description": "MAC address of the detected device"
                    },
                    "rssi": {
                        "type": "string",
                        "description": "Received Signal Strength Indicator (RSSI)"
                    }
                }
            }
        },
        "latitude": {
            "type": "number",
            "description": "Latitude of the device"
        },
        "longitude": {
            "type": "number",
            "description": "Longitude of the device"
        },
        "pdop": {
            "type": "number",
            "description": "Positional Dilution of Precision (PDOP)"
        }
    }
}