{
  "$id": "https://akenza.io/digitalMatter/guppy/lifecycle.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lifecycle",
  "type": "object",
  "properties": {
    "initialVoltage": {
      "title": "Initial battery voltage",
      "type": "number",
      "minimum": 0,
      "maximum": 24,
      "desciption": "Initial battery voltage",
      "unit": "V",
      "hideFromKpis": true
    },
    "uptimeWeeks": {
      "title": "Uptime weeks",
      "type": "integer",
      "unit": "w",
      "description": "Uptime weeks",
      "hideFromKpis": true
    },
    "txCount": {
      "title": "Tx count",
      "type": "integer",
      "description": "Transmission count",
      "hideFromKpis": true
    },
    "tripCount": {
      "title": "Trip count",
      "type": "integer",
      "description": "Trip count",
      "hideFromKpis": true
    },
    "wakeupsPerTrip": {
      "title": "Wakeups per trip",
      "type": "integer",
      "description": "Wakeups per trip",
      "hideFromKpis": true
    }
  },
  "required": [
    "initialVoltage",
    "uptimeWeeks",
    "txCount",
    "tripCount",
    "wakeupsPerTrip"
  ]
}
