  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Lifecycle",
  "type": "object",
  "properties": {
    "batteryVoltage": {
      "title": "Battery voltage",
      "unit": "V",
      "description": "Battery voltage",
