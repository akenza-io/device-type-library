{
  "$id": "https://akenza.io/synetica/enlinkIAQ/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "temperature": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
    },
    "pressure": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/pressure/hPa"
    },
    "humidity": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/humidity/percent"
    },
    "co2": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/co2/ppm"
    },
    "co2e": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/co2/ppm",
      "title": "Estimated CO2",
      "description": "Estimated CO2",
      "hideFromKpis": true
    },
    "bVoc": {
      "title": "B VOC",
      "description": "B VOC",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/tvoc/ppm"
    },
    "iaq": {
      "title": "Indoor Air Quality (TVOCs)",
      "description": "Indoor Air Quality (TVOCs)",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/tvoc/ppm"
    },
    "pm1": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/pm1/mcgm3"
    },
    "pm2_5": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/pm2_5/mcgm3"
    },
    "pm4": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/pm4/mcgm3"
    },
    "pm10": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/pm10/mcgm3"
    },
    "ncPm0_5": {
      "title": "PM0.5 Count",
      "type": "number",
      "unit": "#/cm³",
      "description": "PM0.5 Count in #/cm³"
    },
    "ncPm1": {
      "title": "PM1.0 Count",
      "type": "number",
      "unit": "#/cm³",
      "description": "PM1.0 Count in #/cm³"
    },
    "ncPm2_5": {
      "title": "PM2.5 Count",
      "type": "number",
      "unit": "#/cm³",
      "description": "PM2.5 Count in #/cm³"
    },
    "ncPm4": {
      "title": "PM4.0 Count",
      "type": "number",
      "unit": "#/cm³",
      "description": "PM4.0 Count in #/cm³"
    },
    "ncPm10": {
      "title": "PM10 Count",
      "type": "number",
      "unit": "#/cm³",
      "description": "PM10 Count in #/cm³"
    },
    "pmTps": {
      "title": "Particulate matter typical particle size",
      "type": "number",
      "unit": "μm",
      "description": "Particulate matter typical particle size in μm"
    }
  },
  "required": [
    "temperature",
    "pressure",
    "humidity",
    "co2",
    "co2e",
    "bVoc",
    "iaq",
    "pm1",
    "pm2_5",
    "pm4",
    "pm10",
    "ncPm0_5",
    "ncPm1",
    "ncPm2_5",
    "ncPm4",
    "ncPm10",
    "pmTps"
  ]
}