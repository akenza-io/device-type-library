{
  "$id": "https://akenza.io/elsys/ems-lite/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "temperature": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
    },
    "humidity": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/humidity/percent"
    },
    "accX": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/accX/G",
      "description": "Acceleration on the X-Axis in gravitational acceleration",
      "title": "Acceleration X"
    },
    "accY": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/accY/G",
      "description": "Acceleration on the Y-Axis in gravitational acceleration",
      "title": "Acceleration Y"
    },
    "accZ": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/accZ/G",
      "description": "Acceleration on the Z-Axis in gravitational acceleration",
      "title": "Acceleration Z"
    },
    "pulse1": {
      "title": "Pulse count",
      "description": "Pulse count between events",
      "type": "number",
      "minimum": 0,
      "maximum": 65535
    },
    "pulseAbs": {
      "title": "Pulse count absolute",
      "description": "Pulse count absolute",
      "type": "number"
    },
    "waterleak": {
      "title": "Water leak",
      "description": "Equals true if a waterleak is detected",
      "type": "boolean"
    },
    "reed": {
      "title": "Reed",
      "description": "Equals true if reed is detected",
      "type": "boolean"
    }
  },
  "required": [
    "temperature",
    "humidity",
    "accX",
    "accY",
    "accZ",
    "pulse1",
    "pulseAbs",
    "waterleak",
    "reed"
  ]
}
