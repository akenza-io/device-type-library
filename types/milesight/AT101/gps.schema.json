{
  "$id": "https://akenza.io/milesight/AT101/gps.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "gps",
  "title": "GPS",
  "properties": {
    "latitude": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/location/schema.json#/$defs/latitude/degrees"
    },
    "longitude": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/location/schema.json#/$defs/longitude/degrees"
    },
    "motionStatus": {
      "title": "Motion status",
      "description": "Motion status",
      "type": "string",
      "enum": ["UNKNOWN", "START", "MOVING", "STOP"],
      "hideFromKpis": true
    },
    "geofenceStatus": {
      "title": "Geofence status",
      "description": "Geofence status",
      "type": "string",
      "enum": ["INSIDE", "OUTSIDE", "UNSET", "UNKOWN"],
      "hideFromKpis": true
    }
  }
}
