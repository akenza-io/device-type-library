{
  "$id": "https://akenza.io/milesight/am102/history.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "history",
  "title": "History",
  "properties": {
    "history": {
      "title": "History",
      "description": "An array of historical sensor readings.",
      "type": "array",
      "hideFromKpis": true,
      "items": {
        "type": "object",
        "properties": {
          "timestamp": {
            "title": "Timestamp",
            "type": "number",
            "description": "Unix timestamp of the reading"
          },
          "temperature": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
          },
          "humidity": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/humidity/percent"
          }
        },
        "required": [
          "timestamp",
          "temperature",
          "humidity"
        ]
      }
    }
  },
  "required": [
    "history"
  ]
}