{
  "$id": "https://akenza.io/milesight/ct103/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "current": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/electricity/schema.json#/$defs/currentMax/ampere",
      "title": "Current"
    },
    "total_current": {
      "title": "Total Current",
      "type": "number",
      "unit": "Ah",
      "description": "Total accumulated current consumption.",
      "hideFromKpis": false
    },
    "temperature": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius",
      "description": "Cable temperature measured by the optional external sensor."
    },
    "current_sensor_status": {
      "title": "Current Sensor Status",
      "type": "string",
      "description": "The status of the current sensor.",
      "enum": [
        "normal",
        "over range alarm",
        "read failed"
      ],
      "hideFromKpis": true
    },
    "temperature_sensor_status": {
      "title": "Temperature Sensor Status",
      "type": "string",
      "description": "The status of the temperature sensor.",
      "enum": [
        "normal",
        "over range alarm",
        "read failed"
      ],
      "hideFromKpis": true
    }
  }
}