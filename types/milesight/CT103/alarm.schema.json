{
  "$id": "https://akenza.io/milesight/ct103/alarm.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "alarm",
  "title": "Alarm",
  "properties": {
    "currentMax": {
      "title": "Maximum Current",
      "description": "Maximum current value since the last report.",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/electricity/schema.json#/$defs/current/ampere"
    },
    "currentMin": {
      "title": "Minimum Current",
      "description": "Minimum current value since the last report.",
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/electricity/schema.json#/$defs/current/ampere"
    },
    "currentAlarmStatus": {
      "title": "Current Alarm Status",
      "type": "string",
      "description": "The status of the current alarm.",
      "enum": [
        "CURRENT_THRESHOLD_ALARM",
        "CURRENT_THRESHOLD_ALARM_DISMISS",
        "CURRENT_OVER_RANGE_ALARM",
        "CURRENT_OVER_RANGE_ALARM_DISMISS",
        "CURRENT_THRESHOLD_ALARM_OVER_RANGE_ALARM",
        "CURRENT_THRESHOLD_ALARM_OVER_RANGE_ALARM_DISMISS"
      ],
      "hideFromKpis": true
    },
    "temperatureAlarmStatus": {
      "title": "Temperature Alarm",
      "type": "string",
      "description": "The status of the temperature alarm.",
      "enum": [
        "TEMPERATURE_THRESHOLD_ALARM_RELEASE",
        "TEMPERATURE_THRESHOLD_ALARM"
      ],
      "hideFromKpis": true
    }
  },
  "required": []
}