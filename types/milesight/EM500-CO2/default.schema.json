{
  "$id": "https://akenza.io/milesight/EM500-CO2/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Default",
  "type": "object",
  "properties": {
    "batteryLevel": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100,
      "title": "Battery charge",
      "description": "The battery charge in percent.",
      "unit": "%"
    },
    "temperature": {
      "title": "Temperature",
      "unit": "°C",
      "type": "number",
      "description": "The temperature in °C.",
      "minimum": -3276.5,
      "maximum": 3276.5
    },
    "humidity": {
      "title": "Humidity",
      "unit": "%",
      "type": "number",
      "description": "The relative humidity in %",
      "minimum": 0,
      "maximum": 100
    },
    "pir": {
      "title": "PIR motion",
      "description": "PIR motion sensor measuring activity level",
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "light": {
      "title": "Light",
      "unit": "lux",
      "type": "integer",
      "description": "The light in lux.",
      "minimum": 0,
      "maximum": 65535
    },
    "visibleInfrared": {
      "title": "Visible infrared",
      "type": "integer",
      "description": "Visible infrared",
      "minimum": 0,
      "maximum": 65535
    },
    "infrared": {
      "title": "Infrared",
      "type": "integer",
      "description": "infrared",
      "minimum": 0,
      "maximum": 65535
    },
    "co2": {
      "title": "CO2",
      "unit": "ppm",
      "type": "integer",
      "description": "Carbon Dioxide in ppm",
      "minimum": 0,
      "maximum": 10000
    },
    "tvoc": {
      "title": "TVOC",
      "unit": "ppm",
      "type": "integer",
      "description": "Total Volatile Organic Compunds in ppm",
      "minimum": 0,
      "maximum": 60000
    },
    "pressure": {
      "title": "Pressure",
      "unit": "hpa",
      "description": "Barometric Pressure in hpa",
      "type": "number",
      "minimum": 300,
      "maximum": 1100
    }
  }
}
