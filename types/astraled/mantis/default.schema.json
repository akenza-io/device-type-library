{
  "$id": "https://akenza.io/astraled/mantis/default.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "default",
  "title": "Default",
  "properties": {
    "temperature": {
      "title": "Temperature",
