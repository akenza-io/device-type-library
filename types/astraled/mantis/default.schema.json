    "voc": {
      "title": "VOC",
      "unit": "ppm",
      "type": "integer",
      "description": "Total Volatile Organic Compounds in ppm",
      "minimum": 0,
      "maximum": 60000
    },
    "co2": {
      "title": "CO2",
      "unit": "ppm",
      "type": "integer",
      "description": "Carbon Dioxide in ppm",
