{
  "$id": "https://akenza.io/sensative/ms-guard/lifecycle.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "processingType": "uplink_decoder",
  "topic": "lifecycle",
  "title": "Lifecycle",
  "properties": {
    "batteryLevel": {
      "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/device/schema.json#/$defs/batteryLevel/percent"
    },
    "error": {
      "title": "Error message",
      "description": "Error info which contains: wrong length of RX package",
      "type": "string",
      "hideFromKpis": true
    },
    "historySeqNr": {
      "title": "History sequence number",
      "type": "integer",
      "description": "History sequence number",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "prevHistSeqNr": {
      "title": "Previous history sequence number",
      "type": "integer",
      "description": "Previous history sequence number",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "badConditionsCounter": {
      "title": "Bad conditions counter",
      "type": "integer",
      "description": "Bad conditions counter",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "stackTxFailRebootCount": {
      "title": "Stack TX fail reboot count",
      "type": "integer",
      "description": "Stack TX fail reboot count",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "startUpCount": {
      "title": "Start up count",
      "type": "integer",
      "description": "Start up count",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "watchdogCount": {
      "title": "Watchdog count",
      "type": "integer",
      "description": "Watchdog count",
      "minimum": 0,
      "maximum": 65535,
      "hideFromKpis": true
    },
    "softwareVersion": {
      "title": "Software version",
      "type": "string",
      "description": "Software version",
      "hideFromKpis": true
    }
  },
  "required": ["batteryLevel", "historySeqNr", "prevHistSeqNr"]
}
