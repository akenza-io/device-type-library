{
    "$id": "https://akenza.io/comtac/ms-3/default.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "processingType": "uplink_decoder",
    "topic": "default",
    "title": "Default",
    "properties": {
        "light": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/illuminance/lux"
        },
        "humidity": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/humidity/percent"
        },
        "temperature": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/celsius"
        },
        "accX": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/acceleration/G",
            "title": "Acceleration X-Axis",
            "description": "Acceleration on the X-Axis in gravitational acceleration"
        },
        "accY": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/acceleration/G",
            "title": "Acceleration Y-Axis",
            "description": "Acceleration on the Y-Axis in gravitational acceleration"
        },
        "accZ": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/motion/schema.json#/$defs/acceleration/G",
            "title": "Acceleration Z-Axis",
            "description": "Acceleration on the Z-Axis in gravitational acceleration"
        },
        "gyroX": {
            "title": "X-Gyro velocity",
            "description": "Angular rotational velocity on the X-Gyro in dps",
            "unit": "dps",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "gyroY": {
            "title": "Y-Gyro velocity",
            "description": "Angular rotational velocity on the Y-Gyro in dps",
            "unit": "dps",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "gyroZ": {
            "title": "Z-Gyro velocity",
            "description": "Angular rotational velocity on the Z-Gyro in dps",
            "unit": "dps",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "magnX": {
            "title": "X-Magnetometer velocity",
            "description": "Magnetometer velocity on the X-Magnetometer in gauss",
            "unit": "gauss",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "magnY": {
            "title": "Y-Magnetometer velocity",
            "description": "Magnetometer velocity on the Y-Magnetometer in gauss",
            "unit": "gauss",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "magnZ": {
            "title": "Z-Magnetometer velocity",
            "description": "Magnetometer velocity on the Z-Magnetometer in gauss",
            "unit": "gauss",
            "type": "number",
            "minimum": -32767,
            "maximum": 32767
        },
        "temperatureF": {
            "$ref": "https://raw.githubusercontent.com/akenza-io/device-type-library/main/data-models/environment/schema.json#/$defs/temperature/fahrenheit"
        }
    },
    "required": [
        "light",
        "humidity",
        "temperature",
        "accX",
        "accY",
        "accZ",
        "gyroX",
        "gyroY",
        "gyroZ",
        "magnX",
        "magnY",
        "magnZ"
    ]
}